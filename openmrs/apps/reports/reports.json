   "visitCountAll": {
       "name": "Visit Count Report",
       "type": "aggregation",
       "config": {
           "report":{
               "type": "visits",
                "config": {
                    "applyDateRangeFor": "visitStartDate"
                }
           },
           "rowGroups": [
               "Visit Type"
           ],
           "distinctGroups": [
               "Visit Id"
           ],
            "showTotalRow":true
       }
   },
   "visitCountNB": {
    "name": "Visit Count Report for Non Bhutanese",
