  "visitCountGender": {
         "name": "Visit Count Report Based on Gender",
         "type": "aggregation",
         "config": {
             "report":{
                 "type": "visits",
                  "config": {
                      "applyDateRangeFor": "visitStartDate",
                      "patientAttributes": ["Gender"]
                  }
             },
             "rowGroups": [
                 "Visit Type"
             ],
              "columnGroups": [
                 "Gender"
             ],
             "distinctGroups": [
                 "Patient Identifier"
             ],
              "showTotalRow":true,
              "showTotalColumn":true

         }
     },
