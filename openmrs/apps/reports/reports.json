                       "applyDateRangeFor": "obsDate"
               }
           },
           "rowGroups": ["Visit Type"],
           "distinctGroups": [
               "Patient Identifier"
           ],
            "showTotalRow":false,
            "showTotalColumn":false
       }
            }

        ]
    }
