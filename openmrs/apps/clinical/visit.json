{
    "general":{
        "translationKey": "DASHBOARD_TAB_GENERAL_KEY",
        "displayByDefault": true,
        "printing": {
            "title": "ePIS - Ministry of Health, Bhutan",
            "header": "Visit Summary"
        },
        "defaultSections": true,
        "sections": {
            "pivotTable":{
                "type": "pivotTable",
                "displayOrder": 0,
                "title": "Vitals Flowsheet",
                "config": {
                    "groupBy": "encounters",
                    "obsConcepts": [
                        "Pulse",
                        "Diastolic",
                        "Systolic",
                        "Temperature",
                        "RR",
                        "SPO2",
                        "Height",
                        "WEIGHT",
                        "M/C Days"
                    ],
                    "labConcepts": ""
                }
            },
                 "Radiology Orders": {
              "type": "order",
              "translationKey": "Radiology Orders",
              "config": {
                  "translationKey": "Radiology Orders",
                  "name": "ordersControl",
                  "orderType": "Radiology Order",
                  "conceptNames": [
                      "Radiology Summary"
                  ]
              }
          },
          "Ultrasound Orders": {
              "type": "order",
              "translationKey": "Ultrasound Orders",
              "config": {
                  "translationKey": "Ultrasound Orders",
                  "name": "ordersControl",
                  "orderType": "USG Order",
                  "conceptNames": [
                      "USG Report"
                  ]
              }
          }

        }
    },
    "dischargeSummary":{
        "translationKey": "DASHBOARD_TAB_DISCHARGE_SUMMARY_KEY",
        "printing": {
            "title": "ePIS - Ministry of Health, Bhutan",
            "header": "Discharge Certificate"
        },
        "sections": {
                "patientInformation": {
                "type": "custom",
                "displayType": "Full-Page",
                "displayOrder": 0,
                "config": {
                    "title": "Discharge Summary",
                    "template": "<dischargesummary-header></dischargesummary-header>"
                }
            },

            "diagnoses":{
                "type": "diagnosis",
                "displayOrder": 1,
                "config": {
                    "translationKey" : "DISCHARGE_SUMMARY_DIAGNOSIS_KEY",
                    "showCertainty": false,
                    "showOrder": false,
                    "showDetailsButton": true
                }
            },
            "admissionDetails":{
                "type": "admissionDetails",
                "displayOrder": 2,
                "config": {
                     "showDetailsButton": true,
                     "translationKey": "DISCHARGE_SUMMARY_ADMISSION_DETAILS_KEY"

                }
            },
               "Treatments": {
               "type": "treatment",
                "displayOrder": 3,
                "config": {
                    "translationKey": "Treatment given",
                    "showFlowSheet": true,
                    "showListView": true,
                    "showOtherActive": false,
                    "showDetailsButton": true,
                    "showRoute": true,
                    "showDrugForm": true,
                    "showProvider":false
                 }
             },
            "labInvestigations":{
                "type": "investigationResult",
                "displayOrder": 4,
                "config": {
                    "translationKey":"Investigation done",
                    "showChart": false,
                    "showTable": true,
                    "showNormalLabResults": true,
                    "showCommentsExpanded": true,
                    "showAccessionNotes": true,
                    "numberOfVisits": 10,
                    "initialAccessionCount": 1,
                    "latestAccessionCount": 1
                }
            },
           "instructionOnDischarge":{
                "type": "observation",
                "displayOrder": 6,
                "translationKey": "DISCHARGE_SUMMARY_INSTRUCTION_ON_DISCHARGE_KEY",
                "config": {
                    "conceptNames": [
                    "Instructions on Discharge" 
                    ],
                    "scope": "latest"
                }
            },
            "followUp":{
                "type": "observation",
                "displayOrder": 7,
                "translationKey": "DISCHARGE_SUMMARY_FOLLOW_UP_KEY",
                "config": {
                    "conceptNames": [
                        "Follow up Date",
                        "Follow-Up Notes" 
                    ],
                    "scope": "latest"
                }
            },
            "dischargeSummary": {
                "type": "custom",
                "displayType": "Full-Page",
                "displayOrder": 8,
                "config": {
                    "title": "Discharge Summary",
                    "template": "<discharge-summary></discharge-summary>"
              
                }
            }

        }
    }
}
